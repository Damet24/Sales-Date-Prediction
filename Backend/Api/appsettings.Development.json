{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Debug"
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "Sales Date Prediction",
      "Environment": "Development"
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  }
}
